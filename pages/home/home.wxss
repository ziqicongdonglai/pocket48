/* 导航栏 */
.navbar .item {
  margin-right: 60rpx;
}
/* 隐藏滚动条 */
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}
/*顶部搜索栏 及右边的两个*/
.toprow {
  height: 60rpx;
  padding: 16rpx 0 16rpx 0;
  background-color: #fff;
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-top: 85rpx;
  /* 搜索栏 */
}
.toprow .search {
  display: flex;
  padding: 20rpx 30rpx;
  background: #fff;
  width: 100%;
  /* 右侧的两个 */
}
.toprow .search .input-wrapper {
  background: #f2f2f2;
  width: 80%;
  display: flex;
  justify-content: center;
  align-items: center;
  height: 68rpx;
  border-radius: 68rpx;
  margin-right: 40rpx;
}
.toprow .search .input-wrapper .searchImg {
  width: 32rpx;
  height: 32rpx;
}
.toprow .search .input-wrapper input {
  height: 68rpx;
  font-size: 26rpx;
  width: calc(16%);
  margin-left: 20rpx;
}
.toprow .search .top_right {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}
.toprow .search .top_right image {
  width: 40rpx;
  height: 40rpx;
}
.toprow .search .top_right .gift {
  width: 65rpx;
  height: 70rpx;
  margin-left: 20rpx;
}
/* 轮播图 */
.head_swiper {
  width: 100%;
  height: 400rpx;
  margin-top: 16rpx;
  background-color: #fff;
  margin: 0 auto;
  overflow: hidden;
}
.head_swiper .paddinglr {
  margin: 0 20rpx 0 20rpx;
}
.head_swiper .paddinglr .top-img {
  border-radius: 20rpx;
  width: 100%;
  height: 400rpx;
}
.wx-swiper-dots {
  position: relative;
  left: unset !important;
  right: 0;
  width: 120rpx;
  height: 10rpx;
  padding-right: 10rpx;
}
.wx-swiper-dots.wx-swiper-dots-horizontal {
  margin-bottom: -5rpx;
}
/* 中间功能区 */
.middle_features {
  height: 200rpx;
  background-color: #fff;
  box-sizing: border-box;
  font-size: 26rpx;
  position: relative;
  /* dot定位位置 */
}
.middle_features .features_group {
  display: flex;
  flex-direction: row;
}
.middle_features .features_group .features {
  height: 200rpx;
  background-color: #fff;
  display: flex;
  flex-direction: column;
  flex: 1;
  justify-content: center;
  align-items: center;
}
.middle_features .features_group .features .item_icon {
  width: 70rpx;
  height: 70rpx;
  border-radius: 50%;
  margin-bottom: 30rpx;
}
.middle_features .wx-swiper-dots.wx-swiper-dots-horizontal {
  margin-bottom: -4rpx;
}
.middle_features .wx-swiper-dots {
  display: flex;
  justify-content: center;
  width: 50%;
}
.middle_features .wx-swiper-dot {
  margin: 0 8rpx;
  width: 10rpx;
  height: 7rpx;
  background-color: transparent;
  border-radius: 8rpx;
  background: #efefef;
  transition: all 0.6s ease-in-out;
}
.middle_features .wx-swiper-dot-active {
  width: 17rpx;
  background: #dddddd;
}
/* 热门公演 */
.hot_show {
  margin-top: 10rpx;
  width: 100%;
  height: 386rpx;
  background-color: #fff;
  box-sizing: border-box;
}
.hot_show .content_item_group .content_item {
  padding: 0 20rpx 0 20rpx;
  display: inline-block;
  height: 260rpx;
}
.hot_show .content_item_group .content_item .content image {
  width: 460rpx;
  height: 260rpx;
  border-radius: 20rpx;
}
/* 应援榜 */
.yinyuan {
  margin-top: 10rpx;
  width: 100%;
  height: 330rpx;
  background-color: #fff;
  box-sizing: border-box;
}
.yinyuan .rankings {
  display: flex;
  flex-direction: row;
  margin-top: 20rpx;
}
.yinyuan .rankings .ranking_item {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  flex: 1;
}
.yinyuan .rankings .ranking_item .head {
  position: relative;
  margin-bottom: 20rpx;
}
.yinyuan .rankings .ranking_item .head .head_img {
  overflow: hidden;
  border-radius: 50%;
  width: 130rpx;
  height: 130rpx;
}
.yinyuan .rankings .ranking_item .head .head_img image {
  width: 130rpx;
  height: 130rpx;
}
.yinyuan .rankings .ranking_item .head .rank_img {
  width: 40rpx;
  height: 24rpx;
  position: absolute;
  left: 150rpx;
  top: 104rpx;
}
/* 今日有缘成员 生日祝福 */
.top_bottom {
  width: 100%;
  height: 186rpx;
  background-color: #fff;
  padding-bottom: 56rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.top_bottom image {
  width: 340rpx;
  height: 190rpx;
  margin-right: 5rpx;
}
/* 正在直播 */
.live {
  background-color: #fff;
  height: 550rpx;
  margin-bottom: 3rpx;
}
.live .memberblive {
  margin-top: 50rpx;
}
.live .memberblive .scroll-wrap {
  height: 460rpx;
}
.live .memberblive .scroll-wrap .live_item {
  width: 264rpx;
  height: 366rpx;
  display: inline-block;
  margin: 0 20rpx;
  border: 2rpx solid #f5f2fbd0;
  box-shadow: 4rpx 10rpx 10rpx 5rpx #fbf6fd96;
  border-radius: 8rpx;
}
.live .memberblive .scroll-wrap .live_item .live_bg {
  width: 264rpx;
  height: 366rpx;
  border-radius: 8rpx;
}
.live .memberblive .scroll-wrap .live_item .live_head {
  margin-top: -335rpx;
  display: flex;
  flex-direction: column;
  box-sizing: border-box;
}
.live .memberblive .scroll-wrap .live_item .live_head .head_img {
  width: 120rpx;
  height: 120rpx;
  margin-bottom: 20rpx;
}
.live .memberblive .scroll-wrap .live_item .live_head .head_img image {
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  border: 2rpx solid #c7c2d7;
}
.live .memberblive .scroll-wrap .live_item .live_head .live_title {
  margin-top: 20rpx;
  color: #cccccc;
  font-size: 22rpx;
}
/* 创造营部分 */
.czy {
  margin-top: 90rpx;
  position: relative;
  /*顶部图片*/
  /* 导航栏部分 */
}
.czy .banner_box {
  position: relative;
}
.czy .banner_box .banner {
  width: 100%;
  vertical-align: middle;
}
.czy .banner_box .btn_rule {
  width: 24%;
  position: absolute;
  right: 0;
  top: 1rem;
}
.czy .banner_box .btn_share {
  width: 60rpx;
  height: 60rpx;
  position: absolute;
  right: 200rpx;
  top: 1rem;
}
.czy .nav_bar {
  text-align: center;
  background: url(https://h5.48.cn/2020apppage/creative/img/nav-bg.jpg);
  background-size: 100% 100%;
}
.czy .nav_bar .nav_bar_item {
  width: 33.3%;
  color: #e386b8;
  display: inline-block;
  height: 120rpx;
  line-height: 120rpx;
  font-size: 33rpx;
}
.czy .nav_bar .active {
  color: #fff !important;
  background: -webkit-linear-gradient(left, #ff7595, #ff7595);
}
.czy .content {
  min-height: 50%;
  margin: 0 auto;
  background-color: #fbf4f5;
  padding: 5% 0;
  z-index: 1;
  background: url(https://h5.48.cn/2020apppage/creative/img/bg.jpg);
  background-repeat: repeat-y;
  background-size: 100%;
}
.czy .content .list_member {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.czy .content .list_member .title {
  width: 40%;
}
.czy .content .list_member .arrow_down {
  width: 20%;
}
.czy .content .list_member .member_list {
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
  align-items: center;
}
.czy .content .list_member .member_list .member_item {
  width: 31%;
  display: inline-block;
  text-align: center;
  border-radius: 0.5rem;
  padding: 5% 0;
}
.czy .content .list_member .member_list .member_item .avatar {
  margin: auto;
  width: 132rpx;
  height: 132rpx;
}
.czy .content .list_member .member_list .member_item .avatar image {
  width: 132rpx;
  height: 132rpx;
  vertical-align: middle;
}
.czy .content .list_member .member_list .member_item .member_name {
  color: #000;
  font-size: 26rpx;
  margin: 11rpx 0;
  font-weight: 700;
}
.czy .content .list_member .member_list .member_item .member_btn {
  width: 170rpx;
  height: 70rpx;
  font-size: 26rpx;
  box-sizing: border-box;
  line-height: 70rpx;
}
.czy .content .list_top {
  border-radius: 10rpx;
  z-index: 1;
  width: 90%;
  margin-left: 5%;
}
.czy .content .list_top .title {
  margin-left: 28%;
  width: 44.5%;
}
.czy .content .list_top .rank_list .member_rank {
  padding: 5% 0;
  position: relative;
  color: #000;
}
.czy .content .list_top .rank_list .member_rank .king {
  width: 40rpx;
  height: 34rpx;
  position: absolute;
  left: 8rpx;
  top: 8rpx;
}
.czy .content .list_top .rank_list .member_rank .member_head {
  margin-left: 11rpx;
  width: 132rpx;
  height: 132rpx;
  z-index: 10;
  position: relative;
}
.czy .content .list_top .rank_list .member_rank .member_head .avatar {
  width: 132rpx;
  height: 132rpx;
  position: relative;
}
.czy .content .list_top .rank_list .member_rank .member_head .avatar image {
  width: 132rpx;
  height: 132rpx;
}
.czy .content .list_top .rank_list .member_rank .member_head .index {
  text-align: center;
  background-color: #fbf81a;
  color: #fff;
  position: absolute;
  right: 0;
  bottom: 0;
  z-index: 1;
  border-radius: 32rpx;
  font-weight: bold;
  line-height: 32rpx;
  font-size: 28rpx;
  padding: 2rpx 9rpx;
  background: -webkit-linear-gradient(left, #ff7595, #ff98b0);
}
.czy .content .list_top .rank_list .member_rank .rank_mid {
  display: inline-block;
  vertical-align: middle;
  position: relative;
  z-index: 10;
  width: 55%;
  margin-left: 13rpx;
  font-weight: 700;
  top: -90rpx;
  left: 160rpx;
}
.czy .content .list_top .rank_list .member_rank .line {
  display: inline-block;
  vertical-align: middle;
  z-index: 10;
  border-bottom: 1px solid #eec3db;
  position: absolute !important;
  width: 75%;
  bottom: 0;
  right: 0;
}
.czy .content .tips2 {
  color: #828283;
  text-align: center;
  font-size: 1rem;
  margin-top: 1rem;
}
